{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/app/index.tsx.bak",
    "src/app/(auth)/index.tsx.bak",
    "nativewind-env.d.ts",
    "src/app/(guest-screen)/Login.tsx"
  ]
}
